{
    "family": "fastapi-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::448049829962:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "fastapi-app",
        "image": "REPLACE_ME",  
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  